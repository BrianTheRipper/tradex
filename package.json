{
  "name": "tradex",
  "version": "0.0.1",
  "description": "Crossplatform crypto trading platform (Mac / PC) ",
  "repository": {
    "type": "git",
    "url": "https://github.com/sakhnyuk/tradex.git"
  },
  "main": "index.js",
  "scripts": {
    "start-renderer-dev": "webpack-dev-server --config ./webpack/webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config ./webpack/webpack.main.config.js && electron ./dist/main.dev.js",
    "dev": "rimraf dist && cross-env START_HOT=1 NODE_ENV=development npm run start-renderer-dev"
  },
  "keywords": [
    "cryptocurrency",
    "terminal",
    "trading"
  ],
  "author": "Mikhail Sakhnyuk <its545@yandex.ru>",
  "license": "ISC",
  "devDependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@types/classnames": "^2.2.10",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-virtualized": "^9.21.8",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "electron": "^8.1.1",
    "electron-builder": "^22.4.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^1.1.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^2.0.1",
    "react-hot-loader": "^4.12.20",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.6.1",
    "electron-log": "^4.0.2",
    "electron-window-state": "^5.0.3",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-virtualized": "^9.21.2",
    "reconnecting-websocket": "^4.3.0",
    "redux": "^4.0.5",
    "redux-act": "^1.7.7",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "websocket-crypto-api": "^1.2.4"
  },
  "build": {
    "publish": {
      "provider": "github"
    },
    "productName": "Tradex App",
    "appId": "com.tradex.org",
    "files": [
      "package.json",
      "manifest.json",
      "renderer/index.html",
      "dist/renderer.prod.js",
      "dist/renderer.css",
      "dist/main.prod.js",
      "renderer/charting_library/",
      "assets"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "icon": "./resources/icon.png",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "resources/ico-app.png",
      "signingHashAlgorithms": [
        "sha1",
        "sha256"
      ],
      "publisherName": [
        "tradex"
      ]
    },
    "nsis": {
      "oneClick": "true",
      "perMachine": "true"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "assets",
      "output": "release"
    }
  },
  "homepage": "./"
}
